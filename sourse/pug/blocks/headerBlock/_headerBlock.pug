mixin headerBlock()

	// start headerBlock
	+b.headerBlock.section#headerBlock&attributes(attributes)
		.container-xl-fluid.container
			.alems.d-none.d-sm-block#headElems
				+b.el--1( data-depth=".2" data-scalar-x="14" data-scalar-y="20"): div(data-aos="fade-up"  data-aos-duration="2000")
					img(src="img/el-1.png", alt="")
				+b.el--2( data-depth="0.2"): div(data-aos="fade-up"   data-aos-duration="2000")
					img(src="img/el-2.png", alt="")
				+b.el--3( data-depth="0.3"): div(data-aos="fade-up"  data-aos-duration="2000")
					img(src="img/el-3.png", alt="")
				
				+b.el--4( data-depth="0.1"): div(data-aos="fade-up" data-aos-delay="500"    data-aos-duration="2000" )
					img(src="img/el-4.png", alt="")
				+b.el--5( data-depth="0.2"): div(data-aos="fade-up"  data-aos-delay="500"  data-aos-duration="2000")
					img(src="img/el-5.png", alt="")
				+b.el--6( data-depth="0.3"): div(data-aos="fade-up"  data-aos-delay="500"  data-aos-duration="2000")
					img(src="img/el-6.png", alt="")
				+b.el--7( data-depth="0.1"): div(data-aos="fade-up"  data-aos-delay="500"  data-aos-duration="2000")
					img(src="img/el-7.png", alt="")
			.before( data-aos="zoom-in"  data-aos-delay="500"    data-aos-duration="1000")
					//- <model-3d background-color="#000000" src="/models/3Dmodel.glb"></model-3d>
			div(data-aos="fade-up" data-aos-delay="500" data-aos-duration="1000")
				#container3D.premia-block.d-none.d-md-block
				.d-md-none
					img(src="img/premia.png", alt="").res-i.main-img
			+e.title
				+e.text.animate-text-wrap
					.animate-text( data-aos="fade-up"  data-aos-delay="800")  НАЦИОНАЛЬНАЯ ПРЕМИЯ  ЗА&nbsp;ВКЛАД В&nbsp;РАЗВИТИЕ РОССИЙСКОГО СЕГМЕНТА СЕТИ ИНТЕРНЕТ
				span.h1.animate-text-wrap
					span.animate-text( data-aos="fade-up"  data-aos-delay="800")  Премия 
				span.h1.animate-text-wrap
					span.animate-text( data-aos="fade-up"  data-aos-delay="800")  РУНЕТА 
				span.h1.animate-text-wrap
					span.text-primary.animate-text-wrap
						span.animate-text( data-aos="fade-up"  data-aos-delay="800")  2023
			+e.footer.row
				.col-lg-auto
					+e.row.row( data-aos="fade-up" data-aos-delay="500") 
						.col-lg-auto
							a.btn.btn-primary.btn-circle(href="#") подать заявку
						.col-auto.ttu.order-sm-first
							+e.date.text-sm-end приём заявок
								.strong до 28 НОЯБРЯ
				.col.order-lg-first( data-aos="fade-up" data-aos-delay="500" data-aos-offset="50") 
					+img("img/head-logo-1.png").res-i
					+img("img/head-logo-2.png").res-i

	// end headerBlock

	

mixin headerBlock02(title)
	// start headerBlock
	+b.headerBlock--page.section#headerBlock&attributes(attributes)
		.container-xl-fluid.container
			.alems.d-none.d-sm-block#headElems
				+b.el--1( data-depth=".2" data-scalar-x="14" data-scalar-y="20"): div(data-aos="fade-up"  data-aos-duration="2000")
					img(src="img/el-1.png", alt="")
				+b.el--2( data-depth="0.2"): div(data-aos="fade-up"   data-aos-duration="2000")
