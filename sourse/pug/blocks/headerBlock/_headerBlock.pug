				+b.el--3( data-depth="0.3"): div(data-aos="fade-up"  data-aos-duration="2000")
					img(src="img/el-3.png", alt="")
				
				+b.el--4.--1-page( data-depth="0.1"): div(data-aos="fade-up" data-aos-delay="500"    data-aos-duration="2000" )
					img(src="img/el-10.png", alt="")
				+b.el--5-1( data-depth="0.2"): div(data-aos="fade-up"  data-aos-delay="500"  data-aos-duration="2000")
					img(src="img/el-5.png", alt="")
				+b.el--6( data-depth="0.3"): div(data-aos="fade-up"  data-aos-delay="500"  data-aos-duration="2000")
					img(src="img/el-6.png", alt="")
				+b.el--7-1( data-depth="0.1"): div(data-aos="fade-up"  data-aos-delay="500"  data-aos-duration="2000")
					img(src="img/el-17.png", alt="")
			.before( data-aos="zoom-in"  data-aos-delay="500"    data-aos-duration="1000")
					//- img(src="img/premia.png", alt="").res-i
					//- <model-3d background-color="#000000" src="/models/3Dmodel.glb"></model-3d>
			div(data-aos="fade-up" data-aos-delay="500" data-aos-duration="1000")
				+b.premia-block--page
					img(src="img/head-img-2.png", alt="").res-i
			+e.head
				+e.title
					span.h1.animate-text-wrap
						span.animate-text( data-aos="fade-up"  data-aos-delay="800")= title
				.container
					+e.text-page.d-none.d-lg-block
						.animate-text(data-aos="fade-up" data-aos-delay="800")
							p В&nbsp;этом разделе представлены номинанты Премии Рунета. Кликая по&nbsp;названиям кейсов, вы&nbsp;можете ознакомиться с&nbsp;проектами, представленными на&nbsp;конкурсе в&nbsp;2023 году
			+e.footer.row
				.col-lg-auto
					+e.row.row( data-aos="fade-up" data-aos-delay="500") 
						.col-lg-auto
							a.btn.btn-primary.btn-circle(href="#") стать экспертом
						.col-auto.ttu.order-sm-first.d-none.d-md-block
							+e.date.text-lg-end В этом разделе представлен #[strong экспертный состав Премии 2023]
				.col.order-lg-first.my-0

	// end headerBlock


